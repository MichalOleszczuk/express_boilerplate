{
  "name": "express_boilerplate",
  "version": "1.0.0",
  "description": "ExpressJS + TypeScript with min cloud setup and external configuration",
  "keywords": [
    "expressjs",
    "TypeScript",
    "application.yml"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:MichalOleszczuk/express_boilerplate.git"
  },
  "license": "ISC",
  "author": "Michal Oleszczuk",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p .",
    "postbuild": "npm run repoInfo && copyfiles -f ./git.properties ./build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"configs/**/*.ts\" \"scripts/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test,configs,scripts}/**/*.ts\" --fix",
    "repoInfo": "ts-node ./scripts/gitRepoInfo.ts",
    "seed": "ts-node ./scripts/seeds/index.ts",
    "start": "node ./build/scripts/start.js",
    "start:dev": "nodemon -w src --ext ts --exec ts-node scripts/start.ts",
    "test": "jest --config ./test/unit/jest.config.js",
    "test:contract": "jest --config ./test/contract/jest.config.js",
    "pretest:contract:publish": "npm run repoInfo",
    "test:contract:publish": "ts-node ./test/contract/pact/publish.ts",
    "test:cov": "jest --config ./test/unit/jest.config.js --coverage",
    "test:e2e": "jest --config ./test/e2e/jest.config.js",
    "test:watch": "jest --config ./test/unit/jest.config.js --watch",
    "ts-node": "ts-node",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@influxdata/influxdb-client": "^1.10.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-actuator": "^1.7.0",
    "express-promise-router": "^4.0.1",
    "express-winston": "^4.1.0",
    "git-repo-info": "^2.1.1",
    "http-errors": "^1.8.0",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.21",
    "logform": "^2.2.0",
    "pg": "^8.5.1",
    "prom-client": "^13.1.0",
    "response-time": "^2.3.2",
    "soap": "^0.36.0",
    "swagger-jsdoc": "^6.0.6",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.31",
    "winston": "^3.3.3",
    "winston-logstash-ts": "^0.2.3",
    "winston-transport": "^4.4.0"
  },
  "devDependencies": {
    "@pact-foundation/pact": "^9.15.1",
    "@types/express": "^4.17.11",
    "@types/express-actuator": "^1.6.1",
    "@types/faker": "^5.1.7",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.20",
    "@types/js-yaml": "^4.0.0",
    "@types/lodash": "^4.14.168",
    "@types/pg": "^7.14.10",
    "@types/response-time": "^2.3.4",
    "@types/supertest": "^2.0.10",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "copyfiles": "^2.4.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-sonarqube": "^1.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.4.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "sqlite3": "5.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.2"
  }
}
